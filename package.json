{
  "name": "viking-garage-api",
  "version": "0.5.5",
  "description": "VIKING GARAGE API",
  "scripts": {
    "init": "scripts/initialize.sh",
    "dev": "nodemon --watch src --exec 'ts-node' src/server.ts",
    "test": "npm run lint && NODE_ENV=test npm run build && NODE_ENV=test npm run create:db && NODE_ENV=test mocha -r ts-node/register ./src/tests/**/*.spec.ts",
    "tdd": "npm run build && mocha --watch --watch-extensions ts,tsx -r ts-node/register ./src/tests/**/*.spec.ts",
    "lint": "tslint ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts --exclude ./src/express.ts",
    "postinstall": "npm run create:dist && npm run copy:template && npm run copy:swagger && npm run build",
    "start": "npm run build && node ./dist/server.js",
    "build": "npm run gen:swagger && npm run gen:routes && tsc",
    "copy:swagger": "cp -R ./node_modules/swagger-ui/dist/ ./dist/swagger-ui",
    "copy:template": "cp ./src/express.ts ./node_modules/tsoa/dist/routeGeneration/templates/express.ts",
    "create:db": "ts-node ./src/sequelize/mockups/index.ts",
    "create:dist": "mkdirp dist/swagger-ui",
    "gen:swagger": "$(npm bin)/tsoa swagger",
    "gen:routes": "$(npm bin)/tsoa routes"
  },
  "engines": {
    "node": "8.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michalmikolajczyk/viking-garage-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michalmikolajczyk/viking-garage-api/issues"
  },
  "homepage": "https://github.com/michalmikolajczyk/viking-garage-api#readme",
  "dependencies": {
    "@types/body-parser": "0.0.34",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/mocha": "^2.2.39",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.3.3",
    "@types/passport-jwt": "^2.0.19",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "jsonwebtoken": "^8.1.0",
    "method-override": "^2.3.7",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemailer": "^3.1.3",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.1.2",
    "sequelize": "^4.2.0",
    "sequelize-cli": "^3.0.0-3",
    "stripe": "^5.3.0",
    "swagger-ui": "^2.2.10",
    "ts-node": "^3.1.0",
    "tslint": "^4.4.2",
    "tslint-config-airbnb": "^1.0.0",
    "tsoa": "^1.1.8",
    "typescript": "2.3.4",
    "uuid": "^3.0.1",
    "walk-sync": "^0.3.1"
  }
}
